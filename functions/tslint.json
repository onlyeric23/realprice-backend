{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "object-literal-sort-keys": false,
    "no-console": false,
    "radix": false,
    "no-submodule-imports": [true, "lodash"],
    "no-implicit-dependencies": false,
    "member-access": false
  }
}
